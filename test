local DrRayLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/AZYsGithub/DrRay-UI-Library/main/DrRay.lua"))()
local window = DrRayLibrary:Load("made by.ono101", "Default")

local tab = DrRayLibrary.newTab("Auto Farm", "ImageIdHere")
local tab2 = DrRayLibrary.newTab("Buy", "ImageIdHere")

local player = game.Players.LocalPlayer
local character = player.Character
local humanoidRootPart = character.HumanoidRootPart
local stuffFolder = game.Workspace.Stuff
getgenv().Farm = false

tab.newToggle("Auto Collect", "Toggle! (prints the state)", false, function(toggleState)
if toggleState then
getgenv().Farm = true
while getgenv().Farm do
for _, part in pairs(stuffFolder:GetDescendants()) do
if part:IsA("BasePart") then
humanoidRootPart.CFrame = part.CFrame
wait(0.3) -- added a small delay to avoid infinite loop
end
end
end
else
getgenv().Farm = false
end
end)