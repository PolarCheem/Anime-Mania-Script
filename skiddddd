local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/laagginq/ui-libraries/main/shit-lib/src.lua"))()
local win = lib:Create("Niagra.paid","Test")
local tab1 = win:tab("BigFoot",true)
local cred = win:tab("credits",false)

local player = game.Players.LocalPlayer
local Char = player.Character
local speed = false

tab1:label("Made By Ono")

tab1:button("Test Button",function()
    speed = true
		speedOn()
end)

tab1:toggle("Test Toggle",false,function(v)
    speed = true
		speedOn()
end)

tab1:toggle("Test Toggle2",false,function(v)
    print(v)   
end)

tab1:input("Message","Type Here...",true,function(v)
    print(v)    
end)

local function speedOn()
    while speed do
        Char.Humanoid.WalkSpeed = 50
        task.wait(0)  
    end
end