local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/PolarCheem/Duck-UI/refs/heads/main/aaa"))()
local win = lib:Create("Key system", "Get Key")
local dc = "https://discord.gg/qfAf4yc7yd"
local system = game.CoreGui:WaitForChild("Key system")

local sf_key = "lmaoo"
getgenv().KeyInput = "string"

function Destroy()
    system:Destroy()
end

function CheckKey()
    if sf_key == KeyInput then
        Destroy()
        task.wait(1)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/PolarCheem/Anime-Mania-Script/refs/heads/main/anti"))()
    else
        game.Players.LocalPlayer:Kick("Incorrect key")
    end
end

local tab1 = win:tab("Main", true)

tab1:input("Enter key", "...", false, function(v)
    KeyInput = v
end)

tab1:button("Check Key", function()
    CheckKey()
end)

tab1:button("Get Key", function()
    setclipboard(dc)
    game.Players.LocalPlayer:Kick("Discord Invite copied to clipboard")
end)
