local player = game.Players.LocalPlayer
local char = player.Character
local extra = char.Extra
while true do
char.DefHitbox.Size = Vector3.new(20,20,20)
char.BallControl.Size = Vector3.new(20,20,20)
extra.PowerBuff.Value = 1000
extra.SpeedBuff.Value = 1.18
wait(0)
end
print("done")



local player = game.Players.LocalPlayer
local char = player.Character
local extra = char.Extra
while true do
char.DefHitbox.Size = Vector3.new(10,10,10)
char.BallControl.Size = Vector3.new(12,12,12)
char.ChargeMeter.Value = 100
extra.ControlBuff.Value = 1.1
char.Stamina.Value = 195
wait(0)
end
extra.SpeedBuff.Value = 1.15
print("done")