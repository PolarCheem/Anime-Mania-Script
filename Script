local link = game:HttpGet("https://pastebin.com/raw/ZjTytAmD")
local HWIDTable = { link }
local HWID = game:GetService("RbxAnalyticsService"):GetClientId()

local found = false

for i, v in pairs(HWIDTable) do
    if v:lower() == HWID:lower() then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/PolarCheem/Anime-Mania-Script/refs/heads/main/key%20system"))()
        found = true
        break
    end
end

if not found then
    game.Players.LocalPlayer:Kick("HWID not found.")
end