local allowedGames = {
    9847297509,
    18401497017,
    14004668761,
    13436905139,
    16798791523,
    14165106859,
    7473772280,
}

if table.find(allowedGames, game.PlaceId) then
   loadstring(game:HttpGet("https://raw.githubusercontent.com/PolarCheem/Anime-Mania-Script/main/skibidi"))()
else
    loadstring(game:HttpGet("https://raw.githubusercontent.com/PolarCheem/Anime-Mania-Script/main/universe!"))()
end